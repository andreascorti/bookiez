{
  "name": "bookiez",
  "version": "1.0.0",
  "description": "Book your appointment!",
  "main": "index.js",
  "author": "Andrea Scorti",
  "license": "MIT",
  "scripts": {
    "build:aot:prod": "yarn run clean:dist && yarn run clean:aot && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:aot": "yarn run build:aot:prod",
    "build:dev": "yarn run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "yarn run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "yarn run build:dev",
    "clean:aot": "yarn run del -- 'compiled/**'",
    "clean:dist": "yarn run del -- 'dist/**' '!dist'",
    "clean:install": "yarn set progress=false && yarn install",
    "del": "del",
    "clean": "yarn cache clean && yarn run rimraf -- node_modules doc coverage dist compiled",
    "lint": "yarn run tslint \"src/**/*.ts\"",
    "preclean:install": "yarn run clean",
    "rimraf": "rimraf",
    "server:dev:hmr": "yarn run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "server": "yarn run server:dev",
    "start:hmr": "yarn run server:dev:hmr",
    "start": "yarn run server:dev",
    "test": "yarn run lint && karma start",
    "tslint": "tslint",
    "watch:dev:hmr": "yarn run watch:dev -- --hot",
    "watch:dev": "yarn run build:dev -- --watch",
    "watch:prod": "yarn run build:prod -- --watch",
    "watch:test": "yarn run test -- --auto-watch --no-single-run",
    "watch": "yarn run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "yarn run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/common": "^2.4.7",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.7",
    "@angular/platform-browser": "^2.4.7",
    "@angular/platform-browser-dynamic": "^2.4.7",
    "@angular/router": "^3.4.8",
    "core-js": "^2.4.1",
    "http-server": "^0.9.0",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.9",
    "rimraf": "^2.6.0",
    "rxjs": "^5.1.1",
    "tslib": "^1.5.0",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.8",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.5",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.2.6",
    "@types/whatwg-fetch": "^0.0.33",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.0.4",
    "codelyzer": "^2.0.0",
    "css-loader": "^0.26.1",
    "del-cli": "^0.2.1",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "^1.2.0",
    "protractor": "^5.1.1",
    "raw-loader": "^0.5.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^4.4.2",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.0",
    "webpack-merge": "^2.6.1"
  }
}
